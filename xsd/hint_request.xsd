    <xs:complexType>
      <xs:sequence>
        <xs:element name="LastFetchTime" type="xs:dateTime" />
      </xs:sequence>
      <xs:attribute name="id" type="xs:string" use="required" />
      <xs:attribute name="timestamp" type="xs:dateTime" use="required" />
    </xs:complexType>
  </xs:element>
</xs:schema>